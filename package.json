{
  "name": "node-rfc",
  "description": "nodejs bindings for SAP NW RFC SDK",
  "version": "1.0.0-rc8",
  "homepage": "https://github.com/SAP/node-rfc",
  "author": {
    "name": "Srdjan Boskovic",
    "email": "srdjan.boskovic@sap.com"
  },
  "engines": {
    "node": "6.x || 8.x || 10.x || 11.x",
    "npm": "^6.2.0"
  },
  "cpu": [
    "!arm"
  ],
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "prepublishOnly": "npm ls",
    "preinstall": "npm install node-pre-gyp node-addon-api",
    "install": "node-pre-gyp install --fallback-to-build",
    "wrapper": "tsc -p src/ts",
    "examples": "tsc -p examples/ts",
    "install:dev": "npm i --save-dev @babel/core @types/node compare-versions decimal.js eslint eslint-plugin-jest jest node-gyp prettier-eslint random-bytes typescript",
    "install:prod": "npm i --save @types/bluebird bluebird node-addon-api node-pre-gyp"
  },
  "binary": {
    "module_name": "sapnwrfc",
    "module_path": "./lib/binding/{platform}-{arch}-{node_abi}",
    "host": "https://github.com/SAP/node-rfc/releases/download/",
    "remote_path": "{version}",
    "package_name": "rfc-{platform}-{arch}-{node_abi}.tar.gz"
  },
  "license": "Apache-2.0",
  "keywords": [
    "sap",
    "rfc",
    "rfc sdk",
    "sapnwrfc",
    "sapnw",
    "node",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAP/node-rfc.git"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "files": [
    "lib",
    "src",
    "binding.gyp"
  ],
  "gypfile": true,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/node": "^12.0.2",
    "compare-versions": "^3.4.0",
    "decimal.js": "^10.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.8.0",
    "node-gyp": "^4.0.0",
    "prettier-eslint": "^8.8.2",
    "random-bytes": "^1.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.26",
    "bluebird": "^3.5.4",
    "node-addon-api": "^1.6.3",
    "node-pre-gyp": "^0.13.0"
  }
}
